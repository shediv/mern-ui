    "@types/carbon-components-react": "^7.29.0",
    "@types/carbon__icons-react": "^10.24.0",
    "@types/jest": "^26.0.15",
    "@types/js-cookie": "^2.2.6",
    "@types/jwt-decode": "^3.1.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.32",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "axios": "^0.21.1",
